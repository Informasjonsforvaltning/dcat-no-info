        sh:path dcat:keyword ;
        sh:severity sh:Violation ;
    ], [
        sh:name "endringsdato"@nb , "modified"@en ;
        sh:node :DateOrDateTimeDataType_Shape ;
        sh:maxCount 1 ;
        sh:path dct:modified ;
        sh:severity sh:Violation ;
    ], [
        sh:name "er del av"@nb , "is part of"@en ;
        sh:class modelldcatno:InformationModel ;
        sh:path dct:isPartOf ;
        sh:severity sh:Violation ;
    ], [
        sh:name "er erstattet av"@nb , "is replaced by"@en ;
        sh:class modelldcatno:InformationModel ;
        sh:path dct:isReplacedBy ;
        sh:severity sh:Violation ;
    ], [
        sh:name "erstatter"@nb , "replaces"@en ;
        sh:class modelldcatno:InformationModel ;
        sh:path dct:replaces ;
        sh:severity sh:Violation ;
    ], [
        sh:name "finnes i format"@nb , "has format"@en ;
        sh:class foaf:Document ;
        sh:path dct:hasFormat ;
        sh:severity sh:Violation ;
