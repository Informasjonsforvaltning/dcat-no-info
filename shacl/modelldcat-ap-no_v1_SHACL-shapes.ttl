# -+-+- Shapes for some "general" classes -+-+-+-

:Agent_Shape
    a sh:NodeShape ;
    sh:name "Agent"@en , "Aktør"@nb ;
    sh:property [
        sh:name "navn"@nb , "name"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path foaf:name ;
        sh:severity sh:Violation ;
    ], [
        sh:name "organisasjonsidentifikator"@nb , "identifier"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:identifier ;
        sh:severity sh:Violation ;
    ], [
        sh:name "type"@nb , "type"@en ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:path dct:type ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass foaf:Agent .

:ConceptScheme_Shape
    a sh:NodeShape ;
    sh:name "Thematic Scheme"@en , "Tematisk skjema"@nb ;
    sh:property [
        sh:name "tittel"@nb , "title"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass skos:ConceptScheme .

:LicenceDocument_Shape
    a sh:NodeShape ;
    sh:name "Licence Document"@en , "Lisensdokument"@nb ;
    sh:property [
        sh:name "lisenstype"@nb , "licence type"@en ;
        sh:class skos:Concept ;
        sh:path dct:type ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass dct:LicenseDocument .

:Location_Shape
    a sh:NodeShape ;
    sh:name "Location"@en , "Lokasjon"@nb ;
    sh:property [
        sh:name "område"@nb, "bounding box"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dcat:bbox ;
        sh:severity sh:Violation ;
    ], [
        sh:name "geografisk midtpunkt"@nb , "centroid"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dcat:centroid ;
        sh:severity sh:Violation ;
    ], [
        sh:name "geometri"@nb , "geometry"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path locn:geometry ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass dct:Location .

:PeriodOfTime_Shape
    a sh:NodeShape ;
    sh:name "Period of Time"@en , "Tidsrom"@nb ;
    sh:or (
      [
        sh:property [
          sh:name "sluttdato/tid"@nb , "end date"@en ;
          sh:minCount 1;
          sh:maxCount 1 ;
          sh:path dcat:endDate ;
          sh:severity sh:Violation ;
          sh:shape :DateOrDateTimeDataType_Shape ;
        ]
      ]
      [
        sh:property [
          sh:name "startdato/tid"@nb , "start date"@en ;
          sh:minCount 1;
          sh:maxCount 1 ;
          sh:path dcat:startDate ;
          sh:severity sh:Violation ;
          sh:shape :DateOrDateTimeDataType_Shape ;
        ]
      ]
    ) ;
    sh:message "Enten startdato eller sluttdato må ha verdi"@nb ,
      "Either start date or end date must have value"@en ;
    sh:targetClass dct:PeriodOfTime .

# -+-+- Some union-shapes -+-+-

:DateOrDateTimeDataType_Shape
    a sh:NodeShape ;
    rdfs:comment "Date time date disjunction shape checks that a datatype property receives a date or a dateTime literal" ;
    sh:name "Date time date disjunction"@en , "Enten dato eller datotid"@nb ;
    sh:message "The values must be data typed as either xsd:date or xsd:dateTime"@en ,
       "Verdien må være oppgitt som enten xsd:date eller xsd:dateTime"@nb ;
    sh:or ([
            sh:datatype xsd:date
        ]
        [
            sh:datatype xsd:dateTime
        ]
    ) .

:DcatResource_Shape
    a sh:NodeShape ;
    rdfs:comment "the union of dcat:Catalog and modelldcatno:InformationModel"@en ,
       "union av dcat:Catalog and modelldcatno:InformationModel"@nb ;
    sh:name "dcat:Resource"@nb , "dcat:Resource"@en ;
    sh:message "The node is either a Catalog or an InformationModel"@en ,
       "Noden skal være enten dcat:Catalog eller mdoelldcatno:InformationModel"@nb ;
    sh:or ([
            sh:class dcat:Catalog
        ]
        [
            sh:class modelldcatno:InformationModel
        ]
    ) .


#
# -+-+-+-+- From here, shapes specific for ModellDCAT-AP-NO -+-+-+-
#

:Abstraction_Shape
    a sh:NodeShape ;
    sh:name "Abstraction"@en , "Abstraksjon"@nb ;
    sh:subClassOf modelldcatno:Property ;
    sh:property [
        sh:name "abstraksjon av"@nb , "is abstraction of"@en ;
        sh:maxCount 1 ;
        sh:subClassOf modelldcatno:hasType ;
        sh:path modelldcatno:isAbstractionOf ;
        sh:class modelldcatno:ModelElement ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass modelldcatno:Abstraction .

:Association_Shape
    a sh:NodeShape ;
    sh:name "Association"@en , "Assosiasjon"@nb ;
    sh:subClassOf modelldcatno:Property ;
    sh:property [
        sh:name "refererer til"@nb, "refers to"@en ;
        sh:maxCount 1;
        sh:subClassOf modelldcatno:hasType ;
        sh:path modelldcatno:refersTo ;
        sh:class modelldcatno:ModelElement ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass modelldcatno:Association .

:Attribute_Shape
    a sh:NodeShape ;
    sh:name "Attribute"@en , "Attributt"@nb ;
    sh:subClassOf modelldcatno:Property ;
    sh:or (
      [ sh:property [
          sh:name "har datatype"@nb , "has data type"@en ;
          sh:maxCount 1;
          sh:subClassOf modelldcatno:hasType ;
          sh:path modelldcatno:hasDataType ;
          sh:class modelldcatno:DataType ;
          sh:severity sh:Violation ;
          ];
      ]
      [ sh:property [
          sh:name "har enkeltype"@nb , "has simple type"@en ;
          sh:maxCount 1;
          sh:subClassOf modelldcatno:hasType ;
          sh:path modelldcatno:hasSimpleType ;
          sh:class modelldcatno:SimpleType ;
          sh:severity sh:Violation ;
          ];
      ]
      [ sh:property [
          sh:name "inneholder objekttype"@nb , "contains object type"@en ;
          sh:maxCount 1;
          sh:subClassOf modelldcatno:hasType ;
          sh:path modelldcatno:containsObjectType ;
          sh:class modelldcatno:ObjectType ;
          sh:severity sh:Violation ;
          ];
      ]
    ) ;
    sh:property [
        sh:name "har verdier fra"@nb , "has value from"@en ;
        sh:maxCount 1;
        sh:subClassOf modelldcatno:hasType ;
        sh:path modelldcatno:hasValueFrom ;
        sh:class modelldcatno:CodeList ;
        sh:severity sh:Violation ;
      ] ;
    sh:targetClass modelldcatno:Attribute .

:Catalog_Shape
    a sh:NodeShape ;
    sh:name "Registry Catalogue"@en , "Katalog"@nb ;
    sh:property [
        sh:name "beskrivelse"@nb , "description"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation ;
    ], [
        sh:name "identifikator"@nb , "identifier"@en ;
        sh:nodeKind sh:Literal ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path dct:identifier ;
        sh:severity sh:Violation ;
    ], [
        sh:name "tittel"@nb , "title"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation ;
    ], [
        sh:name "utgiver"@nb , "publisher"@en ;
        sh:class foaf:Agent ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path dct:publisher ;
        sh:severity sh:Violation ;
    ], [
### @@@ svakhet her, burde vært sh:or mellom denne og dct:hasPart dcat:Catalog
        sh:name "modell"@nb , "model"@en ;
        sh:class modelldcatno:InformationModel ;
        sh:minCount 1 ;
        sh:path modelldcatno:model ;
        sh:severity sh:Violation ;
        sh:message "En modellkatalog skal inneholde minst en modell"@nb ,
           "A model catalog shall contain at least one model"@en ;
    ], [
        sh:name "hjemmeside"@nb , "homepage"@en ;
        sh:class foaf:Document ;
        sh:path foaf:homepage ;
        sh:severity sh:Violation ;
    ], [
        sh:name "utgivelsesdato"@nb , "issued (release date)"@en ;
        sh:maxCount 1 ;
        sh:node :DateOrDateTimeDataType_Shape ;
        sh:path dct:issued ;
        sh:severity sh:Violation ;
    ], [
        sh:name "lisens"@nb , "licence"@en ;
        sh:class dct:LicenseDocument ;
        sh:maxCount 1 ;
        sh:path dct:license ;
        sh:severity sh:Violation ;
    ], [
        sh:name "språk"@nb , "language"@en ;
        sh:class dct:LinguisticSystem ;
        sh:path dct:language ;
        sh:severity sh:Violation ;
    ], [
        sh:name "dekningsområde"@nb , "spatial/geographic"@en ;
        sh:class dct:Location ;
        sh:path dct:spatial ;
        sh:severity sh:Violation ;
    ], [
        sh:name "endringsdato"@nb , "modified (last update)"@en ;
        sh:maxCount 1 ;
        sh:node :DateOrDateTimeDataType_Shape ;
        sh:path dct:modified ;
        sh:severity sh:Violation ;
    ], [
        sh:name "temaer"@nb , "themes"@en ;
        sh:class skos:ConceptScheme ;
        sh:path dcat:themeTaxonomy ;
        sh:severity sh:Violation ;
    ], [
        sh:name "har del"@nb , "has part"@en ;
        sh:class dcat:Catalog ;
        sh:path dct:hasPart ;
        sh:severity sh:Violation ;
    ], [
        sh:name "er del av"@nb , "is part of"@en ;
        sh:class dcat:Catalog ;
        sh:maxCount 1 ;
        sh:path dct:isPartOf ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass dcat:Catalog .

:Choice_Shape
    sh:name "Valg"@nb , "Choice"@en ;
    sh:subClassOf modelldcatno:Property ;
    sh:property [
        sh:name "kan ha"@nb , "has member"@en ;
        sh:path modelldcatno:hasSome ;
        sh:class modelldcatno:ModelElement ;
        sh:subClassOf modelldcatno:hasType ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass modelldcatno:Choice .

:CodeElement_Shape
    a sh:NodeShape ;
    sh:name "Code element"@en , "Kodeelement"@nb ;
    sh:property [
        sh:name "i kodeliste"@nb, "in scheme"@en ;
        sh:minCount 1 ;
        sh:class modelldcatno:CodeList ;
        sh:path skos:inScheme ;
        sh:severity sh:Violation
    ], [
        sh:name "kode"@nb , "code"@en ;
        sh:minCount 1;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path skos:notation ;
        sh:severity sh:Violation
    ], [
        sh:name "identifikator"@nb , "identifier"@en ;
        sh:nodeKind sh:Literal ;
        # sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:path dct:identifier ;
        sh:severity sh:Violation
    ], [
        sh:name "anbefalt kodetekst"@nb , "preferred label"@en ;
        # sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path skos:prefLabel ;
        sh:severity sh:Violation
    ], [
        sh:name "begrep"@nb , "subject"@en ;
        sh:maxCount 1 ;
        sh:class skos:Concept ;
        sh:path dct:subject ;
        sh:severity sh:Violation
    ], [
        sh:name "tillatt kodetekst"@nb , "alternative label"@en ;
        sh:nodeKind sh:Literal ;
        sh:path skos:altLabel ;
        sh:severity sh:Violation
    ], [
        sh:name "definisjon"@nb , "definition"@en ;
        sh:nodeKind sh:Literal ;
        sh:path skos:definition ;
        sh:severity sh:Violation
    ], [
        sh:name "eksempel"@nb , "example"@en ;
        sh:nodeKind sh:Literal ;
        sh:path skos:example ;
        sh:severity sh:Violation
    ], [
        sh:name "frarådet kodetekst"@nb , "hidden label"@en ;
        sh:nodeKind sh:Literal ;
        sh:path skos:hiddenLabel ;
        sh:severity sh:Violation
    ], [
        sh:name "merknad"@nb , "note"@en ;
        sh:nodeKind sh:Literal ;
        sh:path skos:note ;
        sh:severity sh:Violation
    ], [
        sh:name "omfangsmerknad"@nb , "scope note"@en ;
        sh:nodeKind sh:Literal ;
        sh:path skos:scopeNote ;
        sh:severity sh:Violation
    ], [
        sh:name "eksklusjonsmerknad"@nb , "exclusion note"@en ;
        sh:nodeKind sh:Literal ;
        sh:path xkos:exclusionNote ;
        sh:severity sh:Violation
    ], [
        sh:name "inklusjonsmerknad"@nb , "inclusion note"@en ;
        sh:nodeKind sh:Literal ;
        sh:path xkos:inclusionNote ;
        sh:severity sh:Violation
    ], [
        sh:name "neste kodeelement"@nb , "next"@en ;
        sh:class modelldcatno:CodeElement ;
        sh:path xkos:next ;
        sh:severity sh:Violation
    ], [
        sh:name "forrige kodeelement"@nb , "previous"@en ;
        sh:class modelldcatno:CodeElement ;
        sh:path xkos:previous ;
        sh:severity sh:Violation
    ], [
        sh:name "toppkonsept til"@nb , "is top concept of"@en ;
        sh:class modelldcatno:CodeList ;
        sh:path skos:topConceptOf ;
        sh:severity sh:Violation
    ] ;
    sh:targetClass modelldcatno:CodeElement .

:CodeList_Shape
    a sh:NodeShape ;
    sh:name "Codelist"@en , "Kodeliste"@nb ;
    sh:subClassOf modelldcatno:ModelElement ;
    sh:property [
        sh:name "ekstern kodeliste"@nb , "code list reference"@en ;
        sh:maxCount 1 ;
        sh:class modelldcatno:CodeList ;
        sh:path modelldcatno:codeListReference ;
        sh:severity sh:Violation
    ] ;
    sh:targetClass modelldcatno:CodeList .

:Collection_Shape
  sh:name "Samling"@nb , "Collection"@en ;
  sh:subClassOf modelldcatno:Property ;
  sh:property [
      sh:name "har medlem"@nb , "has member"@en ;
      sh:path modelldcatno:hasMember ;
      sh:class modelldcatno:ModelElement ;
      sh:subClassOf modelldcatno:hasType ;
      sh:maxCount 1;
      sh:severity sh:Violation ;
  ] ;
  sh:targetClass modelldcatno:Collection .

:Composition_Shape
    a sh:NodeShape ;
    sh:name "Composition"@en , "Komposisjon"@nb ;
    sh:subClassOf modelldcatno:Property ;
    sh:property [
        sh:name "inneholder"@nb , "contains"@en ;
        sh:path modelldcatno:contains ;
        sh:class modelldcatno:ModelElement ;
        sh:maxCount 1;
        sh:subClassOf modelldcatno:hasType ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass modelldcatno:Composition .

:Document_Shape
    a sh:NodeShape ;
    sh:name "Document"@en , "Dokument"@nb ;
    sh:property [
        sh:name "språk"@nb, "language"@en ;
        sh:class dct:LinguisticSystem ;
        sh:maxCount 1 ;
        sh:path dct:language ;
        sh:severity sh:Violation ;
    ], [
        sh:name "tittel"@nb, "title"@en ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation ;
    ], [
        sh:name "format"@nb, "format"@en ;
        sh:class dct:MediaType ;
        sh:maxCount 1 ;
        sh:path dct:format ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass foaf:Document .

