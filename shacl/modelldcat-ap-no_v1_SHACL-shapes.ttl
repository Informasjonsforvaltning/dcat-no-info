        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation ;
    ], [
        sh:name "beskrivelse"@nb , "description"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation ;
    ], [
        sh:name "utgiver"@nb , "publisher"@en ;
        sh:class foaf:Agent ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:path dct:publisher ;
        sh:severity sh:Violation ;
    ], [
        sh:name "tema"@nb , "theme"@en ;
        sh:class skos:Concept ;
        sh:minCount 1 ;
        sh:path dcat:theme ;
        sh:severity sh:Violation ;
    ], [
        sh:name "identifikator"@nb , "identifier"@en ;
        sh:nodeKind sh:Literal ;
        # sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:path dct:identifier ;
        sh:severity sh:Violation ;
    ], [
        sh:name "informasjonsmodellidentifikator"@nb , "information model identifier"@en ;
        sh:nodeKind sh:Literal ;
        sh:maxCount 1;
        sh:path modelldcatno:informationModelIdentifier ;
        sh:severity sh:Violation ;
        sh:message "Informasjonsmodellidentifikatoren skal være Literal"@nb ,
            "The information model identifier shall be Literal"@en ;
    ], [
        sh:name "begrep"@nb , "subject"@en ;
        sh:class skos:Concept ;
        sh:path dct:subject ;
        sh:severity sh:Violation ;
    ], [
        sh:name "inneholder modellelement"@nb , "contains model element"@en ;
        sh:class modelldcatno:ModelElement ;
        sh:path modelldcatno:containsModelElement ;
        sh:severity sh:Violation ;
    ], [
        sh:name "kontaktpunkt"@nb , "contact point"@en ;
        sh:class vcard:Kind ;
        sh:path dcat:contactPoint ;
        sh:severity sh:Violation ;
    ], [
        sh:name "lisens"@nb , "license"@en ;
        sh:class dct:LicenseDocument ;
        sh:maxCount 1 ;
        sh:path dct:license ;
        sh:severity sh:Violation ;
    ], [
